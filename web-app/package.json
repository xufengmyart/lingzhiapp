{
  "name": "lingzhi-ecosystem-app",
  "version": "1.0.0",
  "description": "灵值生态园 - 智能体APP",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "generate-version": "node scripts/generate-version.cjs",
    "prebuild": "npm run generate-version",
    "build": "rm -rf dist && vite build",
    "postbuild": "node scripts/verify-build.cjs",
    "preview": "vite preview",
    "deploy": "bash scripts/deploy.sh",
    "deploy:quick": "bash scripts/deploy.sh --skip-build",
    "deploy:no-backup": "bash scripts/deploy.sh --no-backup",
    "rollback": "bash scripts/deploy.sh --rollback",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "antd": "^6.3.0",
    "axios": "^1.6.8",
    "coze-coding-dev-sdk": "^0.7.15",
    "framer-motion": "^12.34.0",
    "lucide-react": "^0.358.0",
    "quill": "^2.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.22.3",
    "recharts": "^3.7.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-pwa": "^1.2.0"
  }
}
